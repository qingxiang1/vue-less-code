{
  "name": "vue-less-code",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "prettier": "prettier --check  \"src/**/*.{js,ts,json,css,less,scss,vue,html,md}\"",
    "eslint": "eslint --ext .js,.vue ./src vite.config.js ",
    "lint-staged": "lint-staged",
    "commit": "git-cz"
  },
  "repository": "https://github.com/qingxiang1/vue-less-code.git",
  "author": "LQX5731@163.com",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "post-merge": "git submodule update",
      "post-checkout": "git submodule update",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.3",
    "eslint": "^9.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.7",
    "lodash": "^4.17.21",
    "prettier": "^3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "vite": "^5.3.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "dexie": "^4.0.7",
    "element-plus": "^2.7.5",
    "eventemitter3": "^5.0.1",
    "vue": "^3.4.29",
    "vue-draggable-next": "^2.2.1",
    "vue-router": "^4.3.3"
  }
